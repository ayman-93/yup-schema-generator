{
  "name": "yup-schema-generator",
  "version": "1.0.4",
  "description": "This package thakes json or array of objects and convert it to yup schema",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayman-93/yup-schema-generator.git"
  },
  "keywords": [
    "yup",
    "validation",
    "dynamic"
  ],
  "author": "Ayman Almatrudi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ayman-93/yup-schema-generator/issues"
  },
  "homepage": "https://github.com/ayman-93/yup-schema-generator#readme",
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/yup": "^0.26.27",
    "yup": "^0.28.0"
  }
}